@prefix rdf:        <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:       <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:        <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dc:         <http://purl.org/dc/elements/1.1/> .
@prefix dwc:        <http://rs.tdwg.org/dwc/terms/> .
@prefix hispid3:    <http://hiscom.rbg.vic.gov.au/hispid/3/terms/#> .
@prefix abcd:       <http://rs.tdwg.org/abcd/2.06/b/ABCD_2.06b.xsd#> .
@prefix hispid5:    <http://www.chah.org.au/schemas/hispid/5#> .

<>  dc:title    "HISPID Terms 0.1-SNAPSHOT" .
<>  skos:description    "HISPID Terms represents a sea-change in the way Australian Herbaria
manage their data standards. HISPID until now was a data format. From now, it is a list
of terms and definitions ready for use by any data standard."

:institutionCode    a   rdf:Property ;
    rdfs:label  "Institution Code"@en ;
	= dwc:institutionCode ;
	= hispid3:insid ;
	= abcd://Unit/SourceInstitutionID .

:institutionCode    skos:definition    "The Index Herbariorum code for the herbarium supplying the record."@en ;
	skos:example    "MEL"@en     ;
	skos:example    "CANB"@en    .
